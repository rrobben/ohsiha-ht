<div class="navbar">
  <ul>
    <li>
      <%= link_to(t(:table), root_path) %>
    </li>
    <li>
      <%= link_to(t(:chart), chart_path, :'data-turbolinks' => false) %>
    </li>

    <% if logged_user_id %>
      <li>
        <%= link_to(t(:my_profile), edit_user_path(id: logged_user_id)) %>
      </li>
      <li>
        <%= link_to(t(:logout), logout_path) %>
      </li>
    <% else %>
      <li>
        <%#= render(partial: 'sessions/form') %>
        <%= link_to(t(:login), login_path) %>
      </li>
      <li>
        <%= link_to(t(:new_user), new_user_path) %>
      </li>
    <% end %>
  </ul>
</div>